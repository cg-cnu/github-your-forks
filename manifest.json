{
  "name": "Your forks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Show your forks of GitHub projects.",
  "homepage_url": "https://github.com/cg-cnu/your-forks.git",
  "icons": {
    "128": "your_forks_128.png"
  },
  "browser_action": {
    "default_icon": "your_forks_128.png"
  },
  "permissions": [
    "*://github.com/*",
    "*://api.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
